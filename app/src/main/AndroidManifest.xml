<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.juan.firebaseregistro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_eventime"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_eventime"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Menu_Favoritos"></activity>
        <activity android:name=".Deportes_y_Recreaccion"></activity>
        <activity android:name=".Perfil" />
        <activity android:name=".Favoritos" />
        <activity
            android:name=".Menu_Principal"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Principal"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegistroDatos"
            android:screenOrientation="portrait" />
        <activity
            android:name=".fragmento"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>